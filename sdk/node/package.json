{
  "name": "chain-sdk",
  "version": "1.0.0",
  "description": "The Official Node.js SDK for Chain Core",
  "homepage": "https://github.com/chain/chain/tree/main/sdk/node",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chain/chain"
  },
  "author": "Chain Engineering",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.7"
  },
  "scripts": {
    "develop": "npm run build -- --watch",
    "docs": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint \"src/**\"",
    "pretest": "npm run build && babel testSrc --out-dir test",
    "test": "mocha -t 60000",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run lint && npm run build"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "fetch-ponyfill": "^3.0.2",
    "uuid": "~3.0.0"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.13.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0"
  }
}
